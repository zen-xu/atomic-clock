[flake8]
max-line-length = 120
ignore = E501, E203, W503
per-file-ignores =
    __init__.py:F401
    test_*.py:E221
    *.pyi:E302,E704,E301,W504
exclude =
    .git
    __pycache__
    setup.py
    build
    dist
    releases
    .venv
    .tox
    .mypy_cache
    .pytest_cache
    .vscode
    .github
